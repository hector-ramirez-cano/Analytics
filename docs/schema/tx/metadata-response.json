{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MetadataMessage",
  "type": "object",
  "required": ["type", "msg"],
  "properties": {
    "type": {
      "const": "metadata"
    },
    "msg": {
      "type": "object",
      "required": ["metadata", "msg"],
      "properties": {
        "metadata": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "msg": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": {
              "anyOf": [
                { "type": "number" },
                { "type": "string" },
                { "type": "boolean" },
                { "type": "null" }
              ]
            }
          }
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
