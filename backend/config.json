{
  "security": {
    "vault_pass": "ansible/.vault_pass.txt",
    "vault": "ansible/.vault.yml"
  },
  "backend": {
    "model": {
      "private_data_dir": "backend/ansible",
      "playbooks": {
        "fact_gathering": "playbooks/fact_gathering.yml"
      }
    },
    "controller": {
      "cache": {
        "cache_invalidation_s": 60
      },
      "api": {
        "port": 5050
      },
      "postgres": {
        "port": 5432,
        "hostname": "localhost",
        "name": "AnalyticsDB",
        "schema": "postgresql://",
        "$ref": "backend/identity/postgres_db.json"
      },
      "influx": {
        "port": 8086,
        "hostname": "localhost",
        "org": "C5i",
        "schema": "http://",
        "bucket": "analytics",
        "$ref": "backend/identity/influx_db.json"
      }
    }
  }
}