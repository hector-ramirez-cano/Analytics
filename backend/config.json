{
  "debug": {
    "security": {
      "vault_pass": "ansible/.vault_pass.txt",
      "vault": "ansible/.vault.yml"
    },
    "backend": {
      "model": {
        "private_data_dir": "ansible/project",
        "ssh_user": "zaph",
        "cache":{
          "rule_set_cache_invalidation_s": 3600
        }
      },
      "controller": {
        "configure": {
          "enabled": true
        },
        "cache": {
          "cache_invalidation_s": 60
        },
        "syslog": {
          "RFC5424-port":1514
        },
        "fact_gathering":{
          "polling_time_s": 30
        },
        "postgres": {
          "port": 5432,
          "hostname": "localhost",
          "name": "AnalyticsDB",
          "schema": "postgresql://",
          "$ref": "identity/postgres_db.json"
        },
        "influx": {
          "port": 8086,
          "hostname": "localhost",
          "org": "C5i",
          "schema": "http://",
          "bucket": "analytics",
          "$ref": "identity/influx_db.json"
        },
        "telegram": {
          "enabled": true,
          "$ref": "identity/telegram.json"
        }
      }
    }
  },
  "release": {
    "security": {
      "vault_pass": "ansible/.vault_pass.txt",
      "vault": "ansible/.vault.yml"
    },
    "backend": {
      "model": {
        "private_data_dir": "ansible/project",
        "ssh_user": "zaph",
        "cache":{
          "rule_set_cache_invalidation_s": 3600
        }
      },
      "controller": {
        "configure": {
          "enabled": true
        },
        "cache": {
          "cache_invalidation_s": 60
        },
        "syslog": {
          "RFC5424-port":1514
        },
        "fact-gathering":{
          "polling_time_s": 5
        },
        "postgres": {
          "port": 5432,
          "hostname": "postgres_db",
          "name": "AnalyticsDB",
          "schema": "postgresql://",
          "$ref": "identity/postgres_db.json"
        },
        "influx": {
          "port": 8086,
          "hostname": "influx_db",
          "org": "C5i",
          "schema": "http://",
          "bucket": "analytics",
          "$ref": "identity/influx_db.json"
        },
        "telegram": {
          "enabled": true,
          "$ref": "identity/telegram.json"
        }
      }
    }
  }
}
